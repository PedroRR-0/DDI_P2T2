<Window x:Class="traductor.TraductorMain"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:tabData="clr-namespace:traductor"
        mc:Ignorable="d"
        Title="App Traductor" Height="450" Width="800">

    <Grid Name="grid">
        <TabControl>
            <TabItem Header="Inglés-Español">
                <tabData:IngEsp />
            </TabItem>
            <TabItem Header="Español-Inglés">
                <tabData:EspIng />
            </TabItem>
            <TabItem Header="Juego">
                <tabData:Juego />
            </TabItem>
            <TabItem Header="Listado Ordenado">
                <tabData:Listado />
            </TabItem>
        </TabControl>
    </Grid>
</Window>
